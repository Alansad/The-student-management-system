<%- include ../public/header.ejs%>

<div class="container-fluid">
    <div class="row">
        <div class="col-sm-2">

            <a href="/admin/product" class="list-group-item">
                学生信息首页
            </a>
            <a href="/admin/product/add" class="list-group-item">录入学生信息</a>
            <a href="/admin/product/inputScore" class="list-group-item">
                录入学生成绩</a>

            <a href="/admin/product/search" class="list-group-item">查询学生基本信息</a>

            <a href="/admin/product/searchScore" class="list-group-item">
                查询课程成绩</a>
            <a href="/admin/product/searchteacher" class="list-group-item">
                查询某学生的任课老师</a>


        </div>
        <div class="col-sm-10">
            <ol class="breadcrumb">
                <li class="active">学生管理
                </li>
                <li class="active">学生查询
                </li>
            </ol>
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h1>查询补考名单</h1>
                </div>
                <div class="panel-body">
                    <form class="form-inline" action="/admin/product/dobukaostudent" method="get" enctype="multipart/form-data">
                        <div class="form-group">
                            <label for="course">课程</label>
                            <input type="text" class="form-control" name="course" id="course" placeholder="请输入课程名">
                        </div>
                        <div class="form-group">
                            <button type="submit" class="btn btn-default">开始搜索</button>
                        </div>
                    </form>
                </div>
            </div>
            <!--
                列表展示
            -->
            <div class="table-responsive">
                <table class="table table-striped ">
                    <thead>
                    <tr>
                        <th>数量</th>
                        <th>学号</th>
                        <th>姓名</th>
                        <th>专业</th>
                        <th>班级</th>
                        <th>正考成绩</th>
                    </tr>
                    </thead>



                </table>
            </div>

        </div>
    </div>
</div>

</body>
</html>